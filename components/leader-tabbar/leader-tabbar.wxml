<view class="tabbar">
  <view class="tabbar-bg"></view>
  <view class="tabbar-content">
    <view
      class="tab-item {{current === item.key ? 'active' : ''}}"
      wx:for="{{tabs}}"
      wx:key="key"
      data-key="{{item.key}}"
      bindtap="onTap"
    >
      <text class="tab-label">{{item.label}}</text>
    </view>
  </view>
</view>
